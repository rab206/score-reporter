Person json
    name Text
    phone Text
    email Text

Tournament json
    name Text
    startDate UTCTime
    endDate UTCTime
    location Text
    tournamentDirector PersonId
    pitchQty Int
    website Text
    description Text

Game json
    tournamentId TournamentId
    pitchNumber Int
    time UTCTime
    seed1 Int
    seed2 Int
    UniqueGame tournamentId time pitchNumber

Team json
    tournamentId TournamentId
    name Text
    twitterHandle Text Maybe
    seeding Int
    UniqueTeam tournamentId seeding

Score json
    gameId GameId
    teamId TeamId
    scorer Text Maybe
    assister Text Maybe
    time UTCTime
